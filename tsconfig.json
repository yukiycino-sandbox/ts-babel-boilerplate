{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "skipLibCheck": true,
    "allowJs": true,
    "jsx": "react",
    "moduleResolution": "node",
    "noEmit": true,
    "pretty": true,
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "baseUrl": "./src"
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
